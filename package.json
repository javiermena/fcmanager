{
    "name": "fcmanager",
    "version": "1.0.0",
    "description": "A simplified (and football themed) React+Redux version of the Reigns game.",
    "main": "index.js",
    "scripts": {
        "dev": "webpack-dev-server --hot --history-api-fallback --open",
        "clean": "rm -rf dist",
        "prod": "webpack --config webpack.config.prod.babel.js -p",
        "build": "npm run clean && npm run prod",
        "deploy": "npm run build && surge -p dist -d fcmanager.surge.sh"
    },
    "author": "Javier Mena",
    "license": "MIT",
    "homepage": "https://github.com/javiermena/fcmanager",
    "keywords": ["manager", "card", "swipe", "Reigns", "football", "react", "redux", "game"],
    "dependencies": {
        "classnames": "^2.2.5",
        "font-awesome": "^4.7.0",
        "lodash": "^4.17.4",
        "react": "^15.4.2",
        "react-dom": "^15.4.2",
        "react-hammerjs": "^0.5.0",
        "react-hot-loader": "^3.0.0-beta.6",
        "react-redux": "^5.0.3",
        "redux": "^3.6.0"
    },
    "devDependencies": {
        "autoprefixer": "^6.7.7",
        "babel-core": "^6.24.0",
        "babel-eslint": "^7.2.0",
        "babel-loader": "^6.4.1",
        "babel-plugin-transform-object-rest-spread": "^6.23.0",
        "babel-polyfill": "^6.23.0",
        "babel-preset-es2015": "^6.24.0",
        "babel-preset-react": "^6.23.0",
        "babel-register": "^6.24.0",
        "css-loader": "^0.27.3",
        "eslint": "^3.18.0",
        "eslint-config-airbnb": "^14.1.0",
        "eslint-loader": "^1.6.3",
        "eslint-plugin-import": "^2.2.0",
        "eslint-plugin-jsx-a11y": "^4.0.0",
        "eslint-plugin-react": "^6.10.3",
        "extract-text-webpack-plugin": "^2.1.0",
        "favicons-webpack-plugin": "0.0.7",
        "file-loader": "^0.10.1",
        "font-awesome-sass-loader": "^1.0.3",
        "html-webpack-plugin": "^2.28.0",
        "loader-utils": "^1.1.0",
        "node-sass": "^4.5.0",
        "postcss-loader": "^1.3.3",
        "resolve-url-loader": "^2.0.2",
        "sass-loader": "^6.0.3",
        "style-loader": "^0.14.1",
        "url-loader": "^0.5.8",
        "webpack": "^2.2.1",
        "webpack-dev-server": "^2.4.2"
    }
